{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ../../.env -- tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:seed": "dotenv -e ../../.env -- tsx prisma/seed.ts",
    "db:studio": "dotenv -e ../../.env -- prisma studio",
    "db:generate": "dotenv -e ../../.env -- prisma generate"
  },
  "dependencies": {
    "@ai-sdk/openai": "beta",
    "@fastify/cors": "^9.0.1",
    "@prisma/client": "^5.9.0",
    "@trpc/server": "^11.0.0",
    "ai": "beta",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "openai": "^4.26.0",
    "shared": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "prisma": "^5.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
